<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Histórico</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <%- include('partials/header') %>
  <%- include('partials/sidebar') %> <!-- Inclui a sidebar -->
  <div class="content">
    <div class="card">
      <h2>Histórico de Links</h2>
      <ul>
        <% links.forEach(link => { %>
          <li>
            <a href="<%= link.shortUrl %>" target="_blank"><%= link.shortUrl %></a>
            <span>Acessos: <%= link.Accesses.length %></span>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
  <%- include('partials/footer') %>
</body>
</html>